        "blocks": {
          "5541aa05-e453-4a9b-9009-5f9df353f993": {
            "type": "grid-item",
            "settings": {
              "image": "shopify://shop_images/C2.jpg",
              "overlayOpacity": "0.3",
              "overlayColor": "#000000",
              "textColor": "#ffffff",
              "heading": "RECYCLED GLASS",
              "subheading": "<p>100% post consumer recycled glass reduces the carbon footprint of each bottle by 40% compared with virgin glass. Reusable, refillable and recyclable.</p>",
              "link": "",
              "linkText": "",
              "buttonStyle": "2",
              "textAlignment": "top left"
